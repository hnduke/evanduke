{
  "family": "evanduke-web-app",
  "containerDefinitions": [
    {
      "name": "django",
      "image": "hnduke/evanduke:latest",
      "essential": true,
      "memoryReservation": 256,
      "portMappings": [
        {
          "containerPort": 8080
        }
      ]
    },
    {
      "name": "ingress",
      "image": "nginx:alpine",
      "essential": true,
      "memoryReservation": 64,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ]
}
